{
  "name": "maxee-ticketing",
  "version": "1.0.26",
  "description": "hana US kiosk-first > object: ticket",
  "main": "electron/src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hananetworks/kiost-1.git"
  },
  "scripts": {
    "dev:react": "vite --config vite.config.js",
    "build:react": "vite build --config vite.config.js",
    "electron": "cross-env NODE_ENV=development electron .",
    "start": "concurrently \"npm run dev:react\" \"npm run electron\"",
    "prebuild": "npm run build:react",
    "dist": "npm run build:react && electron-builder -p always",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "minho J. and hanna J. and geonwoo L.",
  "license": "MIT",
  "build": {
    "appId": "com.hananetworks.maxee-promotional",
    "productName": "MAXEE_promotional",
    "publish": [
      {
        "provider": "github",
        "releaseType": "release",
        "owner": "hananetworks",
        "repo": "kiost-1"
      }
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "asar": false,
    "compression": "store",
    "files": [
      "dist/**/*",
      "electron/src/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "dll/",
        "to": "dll/",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "ffmpeg.dll",
        "to": "ffmpeg.dll"
      }
    ],
    "extraFiles": [
      {
        "from": ".env",
        "to": "resources/.env"
      },
      {
        "from": "gcloud-key.json",
        "to": "resources/gcloud-key.json"
      },
      {
        "from": "electron/workers",
        "to": "resources/main",
        "filter": [
          "**/*.py"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "resources/icon.ico",
      "artifactName": "${productName} Setup ${version}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "installerIcon": "resources/icon.ico",
      "uninstallerIcon": "resources/icon.ico",
      "installerHeaderIcon": "resources/icon.ico",
      "runAfterFinish": true,
      "shortcutName": "MAXEE Promotional"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.2",
    "7zip-bin": "^5.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^3.0.1",
    "node-gyp": "^11.2.0",
    "node-gyp-build": "^4.8.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.4",
    "vite-plugin-svgr": "^4.5.0"
  },
  "dependencies": {
    "@google-cloud/speech": "^7.2.1",
    "adm-zip": "^0.5.16",
    "auto-launch": "^5.0.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "electron-edge-js": "29.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "node-fetch": "^2.7.0",
    "openai": "^6.3.0",
    "pretendard": "^1.3.9",
    "python-shell": "^5.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "serialport": "^13.0.0",
    "systeminformation": "^5.27.11",
    "uuid": "^13.0.0",
    "webdav": "^4.11.0",
    "webdav-server": "^2.6.2",
    "xlsx": "^0.18.5",
    "xmldom": "^0.6.0"
  }
}